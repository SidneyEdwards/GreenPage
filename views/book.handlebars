<section class="book">
<img src="/images/{{book.name}}" alt="{{book.description}}">
<div class="book-details">
    <h3>{{book.title}}</h3>
    <p>By: {{book.artist}}</p>
    <p>Genre:{{book.genre}}</p>
    <p>Description: {{book.description}}</p>
</div>
</section>

